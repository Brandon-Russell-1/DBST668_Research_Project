SQL> 
SQL> CREATE TABLE Instr_Classes
  2  (
  3  
  4    Instr_Num       INTEGER         NOT NULL,
  5    Sched_Num       INTEGER         NOT NULL,
  6    Course_Num      INTEGER         NOT NULL,
  7    Class_Notes     VARCHAR (1000),
  8    Class_Room      VARCHAR (5),
  9    CONSTRAINT pk_Class  PRIMARY KEY (Instr_Num, Sched_Num),
 10    CONSTRAINT fk_Instr  FOREIGN KEY (Instr_Num)
 11      REFERENCES Instr_List
 12      ON DELETE CASCADE,
 13    CONSTRAINT fk_Sched  FOREIGN KEY (Sched_Num)
 14      REFERENCES Class_Sched
 15      ON DELETE CASCADE,
 16    CONSTRAINT fk_Course FOREIGN KEY (Course_Num)
 17      REFERENCES Course_List
 18      ON DELETE CASCADE
 19  );

Table INSTR_CLASSES created.

SQL> 
