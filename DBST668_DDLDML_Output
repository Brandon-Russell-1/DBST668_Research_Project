SQL> CREATE TABLE Section_Info
  2  (
  3    Section_Num		INTEGER         NOT NULL,
  4    Section_Name	VARCHAR (100),	
  5    Section_Address	VARCHAR (100),	
  6    Section_FoS		VARCHAR (100),	
  7    Section_ZipCode	VARCHAR(10),
  8    CONSTRAINT    pk_section PRIMARY KEY (Section_Num)
  9  );

