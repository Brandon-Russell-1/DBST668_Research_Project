SQL> /* Create trigger */
SQL> /*This trigger will display a message when a row is added to Instr_Classes*/
SQL> CREATE OR REPLACE TRIGGER SchedClass_Trigger AFTER INSERT ON Instr_Classes
  2    FOR EACH ROW
  3      BEGIN
  4        dbms_output.put_line ('A class has been added!!');
  5      END;
  6  /

Trigger SCHEDCLASS_TRIGGER compiled

